void setup() {
	OLED_Setup();

	pinMode(Switch, INPUT);
	
	pinMode(Hour_Button, INPUT_PULLUP);

	pinMode(Minute_Button, INPUT_PULLUP);

	pinMode(Reset_Button, INPUT_PULLUP);

	pinMode(Buzzer, OUTPUT);

	myservo.attach(Servo_Pin);  // attaches the servo on pin 6 to the servo object
	myservo.write(0);
	delay(1000);


	ResetTime();
	

}


void loop() {

	// This value was found experimentally to compensate for error between computed time and real time.  A value of 1 means no error, <1 "speeds up" time on the arduino
	#define time_modifier 0.65090969
	

	
	// How are we going to get accurate-ish time?  Let's use a millis() calculation and a while loop.  delay(1000) was much longer than one second.
	long time_init = millis();

	// Keep looping until elapsed time is 1000 millis
	while (millis() - time_init < 1000*time_modifier)
	{
		// If the current time is greater than the alarm time, go off
		// This doesn't need to be in the loop but it is to make timekeeping more accurate.
		if (Current_Time == Alarm_Time)
		{
			Alarm();
		}

		// If the reset alarm button is pushed, reset the alarm
		if (digitalRead(Reset_Button) == LOW) { ResetTime(true); }
	}
	//Serial.println(Current_Time);
	//Serial.println(Alarm_Time);
	//Serial.println(Current_Time >= Alarm_Time);
	
	// Keep this at the end so we can set both to 00:00:00 for testing alarm
	Current_Time++;
	DisplayTime();

}
 
