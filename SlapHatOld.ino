	display.begin(SSD1306_SWITCHCAPVCC, 0x3C); ///< See datasheet for Address; 0x3D for 128x64, 0x3C for 128x32

	display.display();

	display.setTextSize(2);

	display.setTextColor(WHITE);

	display.setCursor(0, 0);

	showSlapHatLogo();
	StartupNoise();
	delay(5000);

	display.clearDisplay();
	display.display();
}

/// <summary>
/// Uses the Minute and Hours buttons to set time.  When both buttons are pressed, the time is set.
/// 
/// The output format is kind of weird 23:59:00 = 235900, 00:01:00 = 100, 00:00:00 = 0, etc.
/// </summary>
/// <returns>Time in military format (HHMMSS)</returns>
long SetTime(bool Current = true) {

	long SettingTime = 0;
	long Hours = 0;
	long Minutes = 0;

	while (true) {

		
		// Only change time once per loop
		int Minute_Button_State = digitalRead(Minute_Button);
		int Hour_Button_State = digitalRead(Hour_Button);
		
		// Add time for forward
		if (Minute_Button_State == LOW)
		{

			Minutes++;
			if (Minutes >= 60) { Minutes = 0; }
		}

		// Subtract time for backwards
		if (Hour_Button_State == LOW)
		{
			Hours++;
			if (Hours >= 24) { Hours = 0; }
		}

		// Pressing both buttons sets the time
		if (Minute_Button_State == LOW && Hour_Button_State == LOW)
		{
			break;
		}

		// Convert to our format
		SettingTime = Hours * 10000 + Minutes*100;

		if (Current) { Current_Time = SettingTime; }
		if(!Current){ Alarm_Time = SettingTime; }
		DisplayTime();
	}
	return SettingTime;
}

/// <summary>
/// Displays the time in the proper format on the OLED
/// </summary>
/// <param name="Time">Military time int, form HHMMSS</param>
void DisplayTime(void) {

#pragma region current time

	display.setCursor(0, 0);
	display.clearDisplay();

	display.setTextSize(1);
	strcpy_P(buffer, (char*)pgm_read_word(&(string_table[0])));  // Necessary casts and dereferencing, just copy.
	display.println(buffer);
	display.setTextSize(2);
	long Hours = floor(Current_Time / 10000);
	long Minutes = floor((Current_Time - Hours * 10000) / 100);
	long Seconds = Current_Time - Hours * 10000 - Minutes * 100;

	if (Seconds == 60) {
		Seconds = 0;
		Minutes++;
	}

	if (Minutes == 60) {
		Minutes = 0;
		Hours++;
	}

	if (Hours == 24) {
		Hours = 0;
	}

	// Don't need this for alarm because it doesn't change
	Current_Time = Hours * 10000 + Minutes * 100 + Seconds; 

	String Hours_Str = String(Hours);
	String Minutes_Str = String(Minutes);
	String Seconds_Str = String(Seconds);

	// Add a zero if not 2 chars
	if (Hours_Str.length() < 2) {
		Hours_Str = "0" + Hours_Str;
	}

	// Add a zero if not 2 chars
	if (Minutes_Str.length() < 2) {
		Minutes_Str = "0" + Minutes_Str;
	}

	// Add a zero if not 2 chars
	if (Seconds_Str.length() < 2) {
		Seconds_Str = "0" + Seconds_Str;
	}


	display.print(Hours_Str);
	display.print(":");
	display.print(Minutes_Str);
	display.print(":");
	display.println(Seconds_Str);
	display.println();
#pragma endregion




#pragma region alarm time
	display.setTextSize(1);

	strcpy_P(buffer, (char*)pgm_read_word(&(string_table[1])));  // Necessary casts and dereferencing, just copy.
	display.println(buffer);
	display.setTextSize(2);
	
	 Hours = floor(Alarm_Time / 10000);
	 Minutes = floor((Alarm_Time - Hours * 10000) / 100);
	 Seconds = Alarm_Time - Hours * 10000 - Minutes * 100;

	if (Seconds == 60) {
		Seconds = 0;
		Minutes++;
	}

	if (Minutes == 60) {
		Minutes = 0;
		Hours++;
	}

	if (Hours == 24) {
		Hours = 0;
	}


	 Hours_Str = String(Hours);
	 Minutes_Str = String(Minutes);
	 Seconds_Str = String(Seconds);

	// Add a zero if not 2 chars
	if (Hours_Str.length() < 2) {
		Hours_Str = "0" + Hours_Str;
	}

	// Add a zero if not 2 chars
	if (Minutes_Str.length() < 2) {
		Minutes_Str = "0" + Minutes_Str;
	}

	// Add a zero if not 2 chars
	if (Seconds_Str.length() < 2) {
		Seconds_Str = "0" + Seconds_Str;
	}


	display.print(Hours_Str);
	display.print(":");
	display.print(Minutes_Str);
	display.print(":");
	display.println(Seconds_Str);
#pragma endregion

	display.display();
} 

/// <summary>
/// Sets and resets the time
/// </summary>
void ResetTime(bool onlyAlarm = false)
{
	if (onlyAlarm)
	{

		// Set Alarm logo
		showSetAlarmLogo();
		delay(3000);
		BlankScreenTransition();

		// Set the alarm time
		Alarm_Time = SetTime(false);
		
		// Show the alarm set logo
		showAlarmTimeSetLogo();
		delay(1000);
		BlankScreenTransition();
	}

	else {
		// Set Time Logo
		showSetTimeLogo();
		delay(3000);
		BlankScreenTransition();

		// Set the current time
		Current_Time = SetTime();

		// Show curent time set
		showTimeSetLogo();
		delay(1000);
		BlankScreenTransition();


		// Set Alarm logo
		showSetAlarmLogo();
		delay(3000);
		BlankScreenTransition();
		
		// Set the alarm time
		Alarm_Time = SetTime(false);

		// Alarm time set
		showAlarmTimeSetLogo();
		delay(1000);
		BlankScreenTransition();
	}
}



void Alarm(void) {


	long start_time = millis();

	#define start 0
	#define end 180
	#define freq 1000 // buzzer frequency

	// Need to store the time so we can calculate duration
	long mytime = millis();

	// Holding the reset button stops the alarm
	while (mytime < start_time + duration * 1000 * 60 && digitalRead(Reset_Button) != LOW)
	{
		mytime = millis();

		showWakeUpLogo();
		tone(Buzzer, freq);
		myservo.write(start);
		delay(1000);
		
		display.clearDisplay();
		display.display();
		noTone(Buzzer);
		myservo.write(end);
		delay(1000);

		// Go to a different screen when alarm is turned off
		if (digitalRead(Reset_Button) == LOW) {
			display.setCursor(0, 0);
			display.clearDisplay();

			display.setTextSize(1);
			strcpy_P(buffer, (char*)pgm_read_word(&(string_table[6])));  // Necessary casts and dereferencing, just copy.
			display.println(buffer);
			display.display();
			delay(4000);
			break;
		}
	}


}

/// <summary>
/// This may be influenced by the Windows XP startup noise
/// </summary>
void StartupNoise(void) {

	// Times from https://tuneform.com/tools/time-tempo-bpm-to-milliseconds-ms 
	#define BPM 120
	#define quarter 60000/BPM
	#define eighth quarter/2
	#define sixteenth eighth/2

	// Notes from https://pages.mtu.edu/~suits/notefreqs.html
	#define E6 1319
	#define E5 659
	#define B5 988
	#define A5 880

	tone(Buzzer, E6);
	delay(2 * eighth);

	tone(Buzzer, E5);
	delay(1 * sixteenth);

	tone(Buzzer, B5);
	delay(2 * eighth);

	tone(Buzzer, A5);
	delay(2 * quarter);

	tone(Buzzer, E6);
	delay(2 * eighth);

	tone(Buzzer, B5);
	delay(2 * quarter);

	noTone(Buzzer);



}

